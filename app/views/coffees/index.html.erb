<h1>Coffees:</h1>
<br>
<%= link_to "Add New Coffee", new_coffee_path %>
<br>
<br>
<ul>
<% @coffees.all.each do |coffee| %>
    <li><%= link_to coffee.name, coffee_path(coffee) %> - <%= render partial: "stars", locals: {coffee: coffee} %></li>
    Roast: <%= coffee.roast_level %>
    <br><br>
<% end %>
</ul>